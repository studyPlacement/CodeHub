class Solution {
    public int minimumFlips(int n) {
        String s = Integer.toBinaryString(n);
        int i=0; int j = s.length()-1; int cnt = 0;
        while(i < j){
            if(s.charAt(i) != s.charAt(j)){
                cnt++;
            }
            i++; j--;
        }
        return cnt*2;
    }
}
